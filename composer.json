{
    "name": "cloudycity/laravel-builder-macros",
    "type": "library",
    "description": "A set of useful macros for your Query Builder",
    "keywords": ["laravel", "query builder", "macros"],
    "license": "MIT",
    "authors": [
        {
            "name": "cloudycity",
            "email": "cloudycity@foxmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/database": "~5.2.0|~5.3.0|~5.4.0|~5.5.0|^5.6.0|~5.7.0|~5.8.0|~5.9.0",
        "illuminate/support": "~5.2.0|~5.3.0|~5.4.0|~5.5.0|^5.6.0|~5.7.0|~5.8.0|~5.9.0"
    },
    "autoload": {
        "psr-4": { "CloudyCity\\LaravelBuilderMacros\\": "src/" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xuyang\\LaravelBuilderExt\\DatabaseServiceProvider"
            ]
        }
    }
}
